{
  "app": {
    "name": "Linagora Meetings"
  },
  "webserver": {
    "enabled": true,
    "debugPort": {{ hublin_debug_port }},

    "ip": "127.0.0.1",
    "ipv6": "",
    "ssl_ip": "",
    "port": {{ hublin_port }},

    "virtualhosts": [],
    "startupBufferTimeout": 5000
  },
  "wsserver": {
    "enabled": true,
    "port": {{ hublin_port }}
  },
  "webrtc": {
    "enabled": true,
    "level": "debug",
    "appIceServers": [
        {"url": "stun:{{ inventory_hostname }}:3478"},
        {"url": "turn:{{ inventory_hostname }}:3478", "username": "hublin", "credential": "{{ coturn_password.stdout }}"}
    ]
  },
  "log": {
    "file": {
      "enabled": true,
      "filename": "./log/application.log",
      "level": "info",
      "handleExceptions": true,
      "json": false,
      "prettyPrint": true,
      "colorize": false
    },
    "console": {
      "enabled": true,
      "level": "debug",
      "handleExceptions": true,
      "json": false,
      "prettyPrint": true,
      "colorize": true
    }
  },
  "emailInvitation": {
    "templatesDir": "./templates/email"
  },
  "modules": [
    "linagora.io.meetings"
  ]
}
